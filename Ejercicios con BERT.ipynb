    "clf = joblib.load('modelo_clasificacion_productos.pkl')\n",
    "\n",
    "tokenizer = BertTokenizer.from_pretrained('bert-base-uncased')\n",
    "model = BertModel.from_pretrained('bert-base-uncased')\n",
    "\n",
    "app = Flask(__name__)\n",
    "\n",
    "@app.route('/')\n",
    "def home():\n",
    "    return render_template('index.html')\n",
    "\n",
    "@app.route('/clasificar', methods=['POST'])\n",
    "def clasificar_producto():\n",
    "    if request.method == 'POST':\n",
    "        descripcion_producto = request.form['descripcion']\n",
    "\n",
    "        inputs_nuevos = tokenizer(descripcion_producto, padding=True, truncation=True, return_tensors=\"pt\")\n",
    "        outputs_nuevos = model(**inputs_nuevos)\n",
    "        cls_embedding_nuevo = outputs_nuevos.last_hidden_state[:, 0, :].detach().numpy()\n",
    "\n",
    "        codigo_predicho = clf.predict(cls_embedding_nuevo)\n",
    "\n",
    "        return render_template('resultado.html', descripcion=descripcion_producto, codigo=codigo_predicho[0])\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    app.run(debug=True)"
